{
	"name": "@victr/clickdown",
	"version": "1.3.4",
	"exports": "./src/index.ts",
	"lint": {
		"include": ["src"],
		"rules": {
			"tags": ["recommended"]
		}
	},
	"fmt": {
		"indentWidth": 4,
		"lineWidth": 120,
		"semiColons": false,
		"singleQuote": true,
		"useTabs": true
	},
	"compilerOptions": {
		"strict": true,
		"lib": ["deno.ns", "dom", "dom.iterable"]
	},
	"nodeModulesDir": "auto",
	"$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json"
}
